                        <label for="contact_name" class="required">Votre nom :</label>
                        <input type="text" id="contact_name" name="contact_name" autocomplete="off" required="required" data-rule="minlen:8" data-msg="8 caractÃ¨res minimum" placeholder="">
                        <div class="validate"></div>
                    </div>
                </div>
            </div>
        </div>
        <div class="glass-field form-send">
            <div id="contact-go" class="glass-button btn-glss-submit" disabled="">suivant</div>
        </div>
