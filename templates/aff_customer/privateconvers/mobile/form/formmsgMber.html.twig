<div class="msg-memberwebsite_mb">
    {{ form_start(form) }}
    {{ form_errors(form) }}
    <div class="formmsg_mb">
        <span>A :</span><span data-dest="">Tous</span><br>
      {#  {{ form_label(form.subject, "objet:") }}
        {{ form_widget(form.subject, {'attr': {'class': 'iner-subject'}}) }} #}
        {{ form_label(form.content, "votre message :") }}
        {{ form_widget(form.content, {'attr': {'class': 'iner-conversone _newconvers'}}) }}
        <div class="validate"></div>
        <div class="form-send">
            {#    <div class="g-recaptcha" data-sitekey="6LfPKvAUAAAAAIopX1KBsmBtSrI8FekLD-u-s4N5"></div> #}
            <i class="fa fa-telegram" id="postform"></i>
        </div>
    </div>

    {{ form_rest(form) }}
    <div class="loading"></div>
    <div class="error-message"></div>
    {{ form_end(form) }}
</div>
<div class="wait">
    <i class="fa fa-spinner fa-spin"></i>
</div>