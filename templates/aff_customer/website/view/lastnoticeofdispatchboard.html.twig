<div class="flex-notb_v6-fix">
    {% for notice in lastsnotice %}
        {%  if notice.promo is not defined %}
            {%  set post =notice %}
            <div class="last-postall_v6-fix">
                {{include('aff_public/view/noticecity.html.twig')}}
            </div>
        {%  else %}
            {%  set offre = notice %}
            <div class="last-postall_v6-fix">
                {{include('aff_public/view/offrecity.html.twig')}}
            </div>
        {%  endif %}
    {%  else %}
        <div class="flex-board">
            <h3>  Pas de nouvelle affiche actuellement sur {{ city|capitalize }}</h3>
        </div>
    {%  endfor %}
</div>

