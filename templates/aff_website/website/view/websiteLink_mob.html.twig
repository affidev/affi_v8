{%  if website.partnergroup.partners is defined %}
    {%  if website.partnergroup.partners|length > 0 %}

    {#  <h2 class="fz-tit">les liens du {{ website.namewebsite }} </h2> #}

    <div class="af_rel af_flx-nW cent-start">
    {% for rocket in website.partnergroup.partners %}
        <a href="{{ path( 'show_website', {"slugcity":rocket.locality.slugcity,"slug":rocket.slug }) }}" class="part_item_mob" data-website="{{ rocket.id }}">
            {#  <i class="fa fa-times abs-croos"></i> #}
            <div class="notbd-img">
                <div class="iner-img_part_mob" style="background-image: url('{{ asset('spaceweb/template/'~rocket.template.logo.namefile??'img/pinsaffi.png' ) }}')"></div>
            </div>
            <div class="modal_part_mob">
                <h5>{{rocket.namewebsite}}</h5>
            </div>
        </a>
    {%  endfor %}
</div>
    {%  else %}
        <h2 class="fz-tit">pas encore de lien ...</h2>
        <button-f></button-f>
    {% endif %}
{% endif %}


